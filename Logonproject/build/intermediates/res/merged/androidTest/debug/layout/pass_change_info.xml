<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout  android:background="@color/person_bankground_color"
                 android:layout_width="fill_parent"
                 android:layout_height="fill_parent"
                 xmlns:android="http://schemas.android.com/apk/res/android"
        >
        <LinearLayout   android:layout_width="match_parent" android:background="@color/white"  android:orientation="vertical"   android:layout_height="wrap_content">
            <LinearLayout android:layout_width="match_parent"  android:background="@color/gray_303_bg" android:layout_height="4dp"/>
            <LinearLayout android:orientation="horizontal"  android:gravity="center_vertical" android:background="@color/white"   android:id="@+id/person_head_layout" android:paddingLeft="13dp" android:paddingRight="13dp"    android:layout_width="match_parent" android:layout_height="53dp">
                    <EditText   android:id="@+id/pass_old_edt"   android:background="@null" style="@style/headpassThema"  android:layout_width="match_parent"    android:layout_height="wrap_content"  android:password="true"   android:hint="原密码"   />
            </LinearLayout>
            <LinearLayout android:layout_width="match_parent"  android:background="@color/gray_303_bg" android:layout_height="1dp"/>

            <LinearLayout android:orientation="horizontal" android:gravity="center_vertical"  android:id="@+id/person_name_layout"  android:paddingLeft="13dp" android:paddingRight="13dp"   android:background="@color/white"   android:layout_width="match_parent" android:layout_height="53dp">
                    <EditText  android:id="@+id/pass_new_edt" android:gravity="center_vertical"  android:background="@null"   style="@style/headpassThema"  android:layout_width="match_parent" android:layout_height="wrap_content" android:password="true"   android:hint="新密码"  android:layout_centerVertical="true" />
            </LinearLayout>
            <LinearLayout android:layout_width="match_parent"  android:background="@color/gray_303_bg" android:layout_height="1dp"/>

            <LinearLayout android:orientation="horizontal"  android:gravity="center_vertical" android:id="@+id/person_sex_layout" android:background="@color/white"  android:paddingLeft="13dp" android:paddingRight="13dp"   android:layout_width="match_parent" android:layout_height="53dp">
                    <EditText   android:id="@+id/pass_newagin_edt" android:gravity="center_vertical"  android:background="@null"  style="@style/headpassThema"  android:layout_width="match_parent"    android:layout_height="wrap_content"  android:password="true"    android:hint="确认密码"   />
            </LinearLayout>
    </LinearLayout>
</RelativeLayout>

