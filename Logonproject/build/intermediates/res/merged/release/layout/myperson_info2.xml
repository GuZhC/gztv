                                    android:background="@drawable/selector_text_bankground_color"
                                    android:gravity="center"
                                    android:text="保密"
                                    android:textColor="@drawable/selector_bar_text_color"
                                    android:textSize="13sp"/>

                            <RadioButton
                                    android:id="@+id/bar_man_btn"
                                    android:layout_width="30dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:layout_marginRight="5dp"
                                    android:button="@null"
                                    android:gravity="center"
                                    android:text="男"
                                    android:textColor="@drawable/selector_bar_text_color"
                                    android:background="@drawable/selector_text_bankground_color"
                                    android:textSize="13sp"/>

                            <RadioButton
                                    android:id="@+id/bar_woman_btn"
                                    android:layout_width="30dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:button="@null"
                                    android:text="女"
                                    android:textColor="@drawable/selector_bar_text_color"
                                    android:background="@drawable/selector_text_bankground_color"
                                    android:textSize="13sp"/>
                        </RadioGroup>
