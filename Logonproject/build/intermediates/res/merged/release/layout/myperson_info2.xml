                        android:layout_height="wrap_content"
                        android:text="个人资料"
                        android:textSize="17sp"
                        android:textColor="#363636"
                        android:layout_centerInParent="true"/>

                <TextView
                        android:id="@+id/right_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
